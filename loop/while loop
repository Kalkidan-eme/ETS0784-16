count = 0
while count < 5:
    print(f"count: {count}")
    count+=1 #output: "count: 0, count: 1, count: 2, count: 3, count: 4"
x = 0
while x < 2: # outer loop
    y = 0
    while y < 3: # inner loop
        if x != y:
             print(f"x = {x}, y = {y}")
        y += 1
    x += 1 #output: "x = 0, y = 1 x = 0, y = 2 x = 1, y = 0 x = 1, y = 2"
